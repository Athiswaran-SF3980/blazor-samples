        // await for mobile or desktop rendering.
        await Task.Delay(500);
        SampleService.Spinner.Hide();
    }

    private void OnToggleClick()
    {
        isToggled = !isToggled;
        ToggleClass = isToggled ? SampleUtils.AddClass(ToggleClass, SampleUtils.ACTIVE_CLASS) : SampleUtils.RemoveClass(ToggleClass, SampleUtils.ACTIVE_CLASS);
    }
    private void CloseSidebar()
    {
        if (isToggled)
        {
            isToggled = false;
        }
        if (isSettingsToggled)
        {
            isSettingsToggled = false;
        }
    }
}