        SettingsToggleClass = this.isSettingsToggled ? SampleUtils.AddClass(SettingsToggleClass, SampleUtils.ACTIVE_CLASS) : SettingsToggleClass;
        this.categoryOrders.Add(new List
    <string> { "Grids", "File Viewers & Editors", "Layout" });
